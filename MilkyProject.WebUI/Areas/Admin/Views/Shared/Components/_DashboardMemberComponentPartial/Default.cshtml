@model List<MilkyProject.WebUI.Dtos.Member.ResultMemberDto>
<div class="col-md-6">
    <div class="dash_blog">
        <div class="dash_blog_inner">
            <div class="dash_head">
                <h3><span><i class="fa fa-user"></i>Çalışanlarımız</span><span class="plus_green_bt"><a href="#">+</a></span></h3>
            </div>
            <div class="list_cont">
                <p>Çalışan Bilgisi</p>
            </div>
            <div class="msg_list_main">
                <ul class="msg_list">
                    @foreach (var item in Model)
                    {
                        <li>
                            <span><img src="@item.ImageUrl" class="img-responsive" alt="#" /></span>
                            <span>
                                <span class="name_user">@item.Name @item.Surname</span>
                                <span class="msg_user">@item.Title</span>
                                
                            </span>
                        </li>
                    }
                    
                </ul>
            </div>
            <div class="read_more">
                <div class="center"><a class="main_bt read_bt" href="#">Daha Fazlası</a></div>
            </div>
        </div>
    </div>
</div>
